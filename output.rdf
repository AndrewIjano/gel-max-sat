<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="urn:absolute:teste07"
         xmlns="urn:absolute:teste07#">

<owl:Ontology rdf:about="urn:absolute:teste07"/>

<owl:ObjectProperty rdf:about="#hasCause"/>

<owl:ObjectProperty rdf:about="#hasSymptom"/>

<owl:ObjectProperty rdf:about="#isFatherOf">
  <rdfs:subPropertyOf rdf:resource="#isParentOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isParentOf"/>

<owl:ObjectProperty rdf:about="#isGrandfatherOf">
  <owl:propertyChainAxiom>
    <rdf:Description>
      <rdf:first rdf:resource="#isFatherOf"/>
      <rdf:rest>
        <rdf:Description>
          <rdf:first rdf:resource="#isFatherOf"/>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </owl:propertyChainAxiom>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#suspectOf">
  <owl:propertyChainAxiom>
    <rdf:Description>
      <rdf:first rdf:resource="#hasSymptom"/>
      <rdf:rest>
        <rdf:Description>
          <rdf:first rdf:resource="#hasCause"/>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </owl:propertyChainAxiom>
</owl:ObjectProperty>

<owl:Class rdf:about="#Dengue">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
  <rdfs:subClassOf rdf:resource="#Disease"/>
</owl:Class>

<owl:Class rdf:about="#Disease"/>

<owl:Class rdf:about="#DenguePatient">
  <owl:equivalentClass>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#suspectOf"/>
      <owl:someValuesFrom rdf:resource="#Dengue"/>
    </owl:Restriction>
  </owl:equivalentClass>
</owl:Class>

<owl:Class rdf:about="#Fever">
  <rdfs:subClassOf rdf:resource="#Symptom"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCause"/>
      <owl:someValuesFrom rdf:resource="#Dengue"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Symptom">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCause"/>
      <owl:someValuesFrom rdf:resource="#Disease"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#FeverAndRash">
  <rdfs:subClassOf rdf:resource="#Fever"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCause"/>
      <owl:someValuesFrom rdf:resource="#Dengue"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Patient">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasSymptom"/>
      <owl:someValuesFrom rdf:resource="#Symptom"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
  <rdfs:comment>#!pbox-restriction
0 -1
1 1
==
0.05</rdfs:comment>
  <rdfs:comment>#!pbox-restriction
2 1
==
0.7</rdfs:comment>
</rdf:Description>

<rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Nothing">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</rdf:Description>

<Patient rdf:about="#john">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  <hasSymptom rdf:resource="#s1"/>
</Patient>

<FeverAndRash rdf:about="#s1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</FeverAndRash>

<Dengue rdf:about="#dengue1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Dengue>

<Dengue rdf:about="#dengue2">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Dengue>

<Dengue rdf:about="#dengue3">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Dengue>

<Disease rdf:about="#disease1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Disease>

<Dengue rdf:about="#dengue4">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Dengue>

<Symptom rdf:about="#symptom1">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Symptom>

<owl:Axiom>
  <owl:annotatedSource rdf:resource="#FeverAndRash"/>
  <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
  <owl:annotatedTarget>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCause"/>
      <owl:someValuesFrom rdf:resource="#Dengue"/>
    </owl:Restriction>
  </owl:annotatedTarget>
  <rdfs:comment>#!pbox-id 1</rdfs:comment>
</owl:Axiom>

<owl:Axiom>
  <owl:annotatedSource rdf:resource="#Fever"/>
  <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
  <owl:annotatedTarget>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCause"/>
      <owl:someValuesFrom rdf:resource="#Dengue"/>
    </owl:Restriction>
  </owl:annotatedTarget>
  <rdfs:comment>#!pbox-id 0</rdfs:comment>
</owl:Axiom>

<owl:Axiom>
  <owl:annotatedSource rdf:resource="#Dengue"/>
  <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
  <owl:annotatedTarget rdf:resource="http://www.w3.org/2002/07/owl#Nothing"/>
  <rdfs:comment>#!pbox-id 2</rdfs:comment>
</owl:Axiom>


</rdf:RDF>
